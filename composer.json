{
    "name":"friendsofcake/crud-users",
    "description":"Adds basic users support to CakePHP applications that use the Crud plugin.",
    "type":"cakephp-plugin",
    "keywords":[
        "cakephp",
        "crud",
        "users"
    ],
    "homepage":"https://github.com/FriendsOfCake/crud-users",
    "license":"MIT",
    "authors":[
        {
            "name":"Jad Bitar",
            "role":"Author",
            "homepage":"http://jadb.io/"
        },
        {
            "name":"ADmad",
            "role":"Contributor",
            "homepage":"https://github.com/ADmad"
        }
    ],
    "require":{
        "cakephp/cakephp":"^5.0",
        "friendsofcake/crud":"^7.0"
    },
    "require-dev":{
        "phpunit/phpunit":"^10.1 || ^11.0",
        "friendsofcake/cakephp-test-utilities":"^3.0"
    },
    "autoload":{
        "psr-4":{
            "CrudUsers\\":"src"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "CrudUsers\\Test\\":"tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
