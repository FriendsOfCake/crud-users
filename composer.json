{
    "name":"friendsofcake/crud-users",
    "description":"Adds basic users support to CakePHP 3 applications that use the Crud plugin.",
    "type":"cakephp-plugin",
    "keywords":[
        "cakephp",
        "crud",
        "users"
    ],
    "homepage":"https://github.com/FriendsOfCake/crud-users",
    "license":"MIT",
    "authors":[
        {
            "name":"Jad Bitar",
            "role":"Author",
            "homepage":"http://jadb.io/"
        },
        {
            "name":"ADmad",
            "role":"Contributor",
            "homepage":"https://github.com/ADmad"
        }
    ],
    "require":{
        "php": ">=5.6",
        "cakephp/cakephp":"~3.0",
        "friendsofcake/crud":"~5.0"
    },
    "require-dev":{
        "phpunit/phpunit":"*",
        "friendsofcake/cakephp-test-utilities":"dev-master"
    },
    "autoload":{
        "psr-4":{
            "CrudUsers\\":"src"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "CrudUsers\\Test\\":"tests"
        }
    }
}
